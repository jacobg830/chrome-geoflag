{
  "name": "GeoFlag",
  "version": "1.0.39",
  "manifest_version": 2,
  "description": "@_MSG_appDescription_@",
  "homepage_url": "https://github.com/jacobg830/chrome-geoflag",
  "minimum_chrome_version": "37",
  "icons": {
    "16": "img/icons/icon16.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "components/ipaddr/lib/ipaddr.js",
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": "img/icons/icon19.png",
    "default_title": "GeoFlag",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "<all_urls>",
    "webRequest"
  ]
}
